{
    "manifest_version": 2,
    "name": "Better Browse for Netflix",
    "short_name": "Better Browse",
    "version":"1.0.3",
    "description": "Browse more Netflix categories",
    "icons": {
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "author": "Jamie Lee",
    "content_scripts": [
        {
        "matches": [
            "https://www.netflix.com/*"
        ],
        "css": [
            "style.css"
        ],
        "js": [
            "jquery-3.1.0.min.js", "scripts.js"
        ],
         "run_at": "document_start"
        }
    ]
}